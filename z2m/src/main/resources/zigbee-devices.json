  {
    "name": "Door Sensor",
    "model": [
      "MCCGQ11LM"
    ],
    "icon": "fas fa-door-open",
    "iconColor": "#A41CE2",
    "widgets": [
      {
        "type": "display",
        "name": "door_sensor",
        "icon": "fas fa-door-open",
        "layout": "{'c':[{'w':15},{'w':45},{'w':40}],'r':[{'cell':[{'ha':'right','va':'middle','v':'icon','cs':1,'rs':1,'w':15},{'ha':'right','va':'middle','v':'name','cs':1,'rs':1,'w':45},{'ha':'center','va':'middle','v':'value','cs':1,'rs':1,'w':40}]}]}",
        "props": [
          {
            "name": "contact",
            "valueConverter": "return value == true ? 'opened' : 'closed';",
            "iconThreshold": [
              {
                "op": "eq",
                "value": true,
                "target": "fas fa-door-open"
              },
              {
                "op": "eq",
                "value": false,
                "target": "fas fa-door-closed"
              }
            ],
            "iconColorThreshold": [
              {
                "op": "eq",
                "value": true,
                "target": "#3A75B5"
              },
              {
                "op": "eq",
                "value": false,
                "target": "#A8A8A8"
              }
            ]
          }
        ],
        "options": {
          "chart": {
            "source": {
              "kind": "property",
              "value": "contact"
            },
            "stepped": "True",
            "fillEmptyValues": true,
            "aggregateFunc": "Last",
            "opacity": 25,
            "height": 50,
            "smoothing": false,
            "min": 0,
            "max": 1
          }
        }
      }
    ]
  },
