  {
    "name": "Temperature and Humidity Sensor",
    "model": [
      "WSDCGQ11LM",
      "WSDCGQ01LM"
    ],
    "icon": "fas fa-cloud",
    "iconColor": "#57A4D1",
    "widgets": [
      {
        "type": "display",
        "name": "temperature_sensor",
        "icon": "fas fa-temperature-full",
        "props": [
          {
            "name": "temperature"
          },
          {
            "name": "humidity"
          },
          {
            "name": "pressure"
          }
        ],
        "requests": [
          {
            "name": "min",
            "value": 16,
            "type": "number",
            "title": "Minimum expected temperature",
            "target": "options.chart.min"
          },
          {
            "name": "max",
            "value": 26,
            "type": "number",
            "title": "Maximum expected temperature",
            "target": "options.chart.max"
          }
        ],
        "options": {
          "chart": {
            "source": {
              "kind": "property",
              "value": "temperature"
            },
            "height": 100,
            "fillEmptyValues": true,
            "min": 16,
            "max": 26,
            "opacity": 25
          }
        }
      }
    ]
  },
